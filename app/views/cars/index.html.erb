<h1>Listing cars</h1>

<table>
  <tr>
    <th>Year</th>
    <th>Make</th>
    <th>Model</th>
	<th>Type</th>
  </tr>

<% @cars.each do |car| %>
  <tr>
	<td><%= car.year %></td>
	<td><%= car.make %></td>
	<td><%= car.model %></td>
	<td><%= car.vehicle_type %></td>
    <td><%= link_to 'Show', car %> | </td>
    <td><%= link_to 'Edit', edit_car_path(car) %> | </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car', new_car_path %>

